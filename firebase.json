{
	"hosting": {
		"public": "build",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**",
			"public/Games"
		],
		"redirects": [],
		"rewrites": [
			{
				"source": "/chatgpt",
				"function": "app"
			},
			{
				"source": "/dalle",
				"function": "app"
			}
		]
	},
	"functions": [
		{
			"source": "backend",
			"codebase": "default",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log"
			]
		}
	]
}